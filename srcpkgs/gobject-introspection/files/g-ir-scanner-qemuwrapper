#!/bin/sh
# Use a modules directory which doesn't exist so we don't load random things
# which may then get deleted (or their dependencies) and potentially segfault
export GIO_MODULE_DIR=${DULGE_CROSS_BASE}/gio/modules-dummy

/usr/bin/qemu-${DULGE_TARGET_QEMU_MACHINE}-static ${GIR_EXTRA_OPTIONS} \
			-L ${DULGE_CROSS_BASE} \
			-E LD_LIBRARY_PATH="${DULGE_CROSS_BASE}/usr/lib:.libs:${GIR_EXTRA_LIBS_PATH}" \
			"$@"
